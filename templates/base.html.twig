<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}


        <link rel="stylesheet" href="/assets/css/base.css">
        <link rel="stylesheet" href="/assets/css/variables.css">

        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>

        {% block header %}
        <div class="header">
            <img class="logo" src="/uploads/Logo_Chelsea.png" alt="Logo de Chelsea">
            <div class="nav">
                <p>Training</p>
                <p>Team</p>
                <p>Logout</p>
            </div>
        </div>
        {# <a href="{{ path('index') }}">Training</a> #}
        {# <a href="{{ path('index') }}">Team</a> #}

        {% endblock %}

        {% block body %}{% endblock %}
    </body>
</html>
